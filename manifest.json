{
    "manifest_version": 3,
    "name": "YouTube Specific Time Jumper",
    "version": "1.0.0",
    "description": "Jump to specific time in the video",
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
}
