{
    "manifest_version": 3,
    "name": "YouTube Specific Time Jumper",
    "version": "1.0.0",
    "description": "Jump to specific time in the video",
    
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "web_16.png",
        "48": "web_48.png",
        "128": "web_128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "web_16.png",
      "48": "web_48.png",
      "128": "web_128.png"
    }
}
